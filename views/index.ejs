<div class="container-fluid hidden-xs">
	<div class="row">
		<div class="col-xl-10 col-xl-offset-1">
			
			<div id="bookCarousal" class="carousel slide" data-ride="carousel">
				<!-- Indicators -->
				<ol class="carousel-indicators">
					<li data-target="#bookCarousal" data-slide-to="0" class="active"></li>
					<li data-target="#bookCarousal" data-slide-to="1"></li>
					<li data-target="#bookCarousal" data-slide-to="2"></li>
					<li data-target="#bookCarousal" data-slide-to="3"></li>
				</ol>
				
				<!-- Wrapper for slides -->
				<div class="carousel-inner">
					<div class="item active">
						<img src="images/books1.jpg" alt="i" class="img">
					</div>
					
					<div class="item">
						<img src="images/books2.jpeg" alt="img3" class="img">
					</div>
					
					<div class="item">
						<img src="images/books3.jpeg" alt="img3" class="img">
					</div>
					
					<div class="item">
						<img src="images/book4.jpg" alt="img3" class="img">
					</div>
				</div>
				
				<!-- Left and right controls -->
				<a class="left carousel-control" href="#bookCarousal" data-slide="prev">
					<span class="glyphicon glyphicon-chevron-left"></span>
					<span class="sr-only">Previous</span>
				</a>
				<a class="right carousel-control" href="#bookCarousal" data-slide="next">
					<span class="glyphicon glyphicon-chevron-right"></span>
					<span class="sr-only">Next</span>
				</a>
			</div>
		</div>
	</div>
</div>
